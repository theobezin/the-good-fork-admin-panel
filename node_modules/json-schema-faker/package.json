{
  "name": "json-schema-faker",
  "version": "0.4.7",
  "description": "JSON-Schema + fake data generators",
  "homepage": "http://json-schema-faker.js.org",
  "main": "index.js",
  "scripts": {
    "test:lint": "tslint ts/**/*.ts && eslint lib",
    "test:unit": "jasmine-node spec/unit --noStackTrace --captureExceptions",
    "test:schema": "jasmine-node spec/schema --coffee --noStackTrace --captureExceptions",
    "test": "npm run test:lint && npm run test:unit && npm run test:schema",
    "dev": "jasmine-node spec/schema --coffee --verbose --autoTest --watchFolders lib",
    "cover": "istanbul cover --root lib --x '**/spec/**' -- jasmine-node --coffee spec",
    "cover:up": "codecov --file=coverage/lcov.info --disable=gcov -e TRAVIS_NODE_VERSION",
    "dist": "npm run build && npm run build:dist",
    "build": "tarima -qf VERSION=`./build/VERSION`",
    "build:dist": "npm run build && node build/dist.js",
    "typedoc": "typedoc --out docs/html ts/ --module commonjs",
    "graphviz": "madge lib --dot > structure.gv"
  },
  "contributors": [
    {
      "name": "Alvaro Cabrera",
      "email": "pateketrueke@gmail.com"
    },
    {
      "name": "Tomasz Ducin",
      "email": "tomasz@ducin.it",
      "url": "http://ducin.it"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/json-schema-faker/json-schema-faker"
  },
  "bugs": "https://github.com/json-schema-faker/json-schema-faker/issues",
  "license": "MIT",
  "tarima": {
    "from": "ts",
    "output": "lib",
    "bundle": true,
    "ignore": [
      "**/*.d.ts"
    ],
    "filter": [
      "!*/*/**"
    ],
    "rename": [
      "ts/**:{fullpath/1}"
    ],
    "bundleOptions": {
      "rollup": {
        "bundle": "JSONSchemaFaker",
        "format": "cjs",
        "interop": false
      }
    }
  },
  "dependencies": {
    "chance": "^1.0.11",
    "deref": "^0.7.0",
    "faker": "^4.1.0",
    "randexp": "^0.4.6",
    "tslib": "^1.8.0"
  },
  "devDependencies": {
    "clone": "^2.1.0",
    "codecov": "^3.0.0",
    "eslint": "^4.14.0",
    "fs-extra": "^5.0.0",
    "glob": "^7.1.1",
    "google-closure-compiler-js": "^20171203.0.0",
    "istanbul": "^0.4.5",
    "jasmine-node": "2.0.0-beta4",
    "jayschema": "^0.3.1",
    "lodash.template": "^4.4.0",
    "rollup": "^0.53.0",
    "rollup-plugin-commonjs": "^8.2.1",
    "rollup-plugin-node-resolve": "^3.0.0",
    "seedrandom": "^2.4.3",
    "semver": "^5.3.0",
    "tarima": "^4.0.1",
    "ts-node": "^4.1.0",
    "tslint": "^5.8.0",
    "tv4": "^1.2.7",
    "typedoc": "^0.9.0",
    "typescript": "^2.1.1",
    "uglify-js": "^3.3.2",
    "z-schema": "^3.18.1"
  },
  "keywords": [
    "json",
    "jsonschema",
    "fake",
    "mocks"
  ]
}
